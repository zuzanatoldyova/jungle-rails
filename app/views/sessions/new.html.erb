<h1>Login</h1>

<%= form_tag '/sessions' do %>

  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Login", class: 'btn btn-primary'%>

<% end %>